{
  "name": "my-gatsby-starter",
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Daniel Martinez",
  "dependencies": {
    "@mdx-js/mdx": "^1.1.5",
    "@mdx-js/react": "1.1.5",
    "@mdx-js/tag": "^0.20.3",
    "autoprefixer": "^9.6.1",
    "gatsby": "^2.13.50",
    "gatsby-image": "^2.2.8",
    "gatsby-plugin-manifest": "^2.2.4",
    "gatsby-plugin-mdx": "^1.0.0",
    "gatsby-plugin-offline": "^2.2.4",
    "gatsby-plugin-postcss": "^2.1.2",
    "gatsby-plugin-purgecss": "^4.0.0",
    "gatsby-plugin-react-helmet": "^3.1.3",
    "gatsby-plugin-sass": "^2.1.4",
    "gatsby-plugin-sharp": "^2.2.9",
    "gatsby-source-filesystem": "^2.1.8",
    "gatsby-transformer-remark": "^2.6.10",
    "gatsby-transformer-sharp": "^2.2.5",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "tailwindcss": "1.0.6"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.6.0",
    "prettier": "^1.18.2",
    "stylelint": "^10.1.0",
    "stylelint-config-css-modules": "^1.4.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-order": "^3.0.1",
    "stylelint-scss": "^3.9.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "start": "npm run develop",
    "develop": "gatsby develop",
    "build": "rm -rf public && rm -rf .cache && gatsby build",
    "serve": "gatsby serve",
    "format": "prettier --write src/**/*.{js,jsx,json,css,md,yaml}",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "lint:fix": "npm run lint:es && npm run lint:style",
    "lint:es": "eslint --ext .js,.jsx --ignore-pattern public . --fix",
    "lint:style": "stylelint \"src/**/*.{css,scss,js,jsx}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
